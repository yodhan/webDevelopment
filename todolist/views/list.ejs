<%-include("header");-%>
        <div class="box" id="heading">
            <h1><%=listTitle%></h1>
        </div>

        <div class="box">
            <%for( var i=0;i<list.length;i++){%>
                <div class="item">
                    <input type="checkbox">
                    <p><%=list[i]%></li></p>
                </div>
            <%}%>
        
            <form class="item" action="/" method="post">
                <input type="text" name="val1" placeholder="Item" autocomplete="off">
                <button type="submit" name="list" value=<%= listTitle%>>+</button>
            
            </form>
        </div>    
<%-include("footer");-%>